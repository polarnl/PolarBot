generator client {
  provider = "prisma-client-js"
  output   = "../src/prisma-client/"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model server {
  id        String   @id @map("_id") @db.String
  name      String
  modRules  Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}